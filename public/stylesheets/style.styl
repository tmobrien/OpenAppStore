@font-face {
    font-family: 'FFFTusjBold';
    src: url('/fonts/FFF_Tusj-webfont.eot');
    src: url('/fonts/FFF_Tusj-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/FFF_Tusj-webfont.woff') format('woff'),
         url('/fonts/FFF_Tusj-webfont.ttf') format('truetype'),
         url('/fonts/FFF_Tusj-webfont.svg#FFFTusjBold') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'CabinRegular';
    src: url('/fonts/Cabin-Regular-webfont.eot');
    src: url('/fonts/Cabin-Regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/Cabin-Regular-webfont.woff') format('woff'),
         url('/fonts/Cabin-Regular-webfont.ttf') format('truetype'),
         url('/fonts/Cabin-Regular-webfont.svg#CabinRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

.mosaic-block
	float left
	position relative
	overflow hidden
	width 132px
	height 164px
	margin 0
	border 1px solid #333
	background-color #555

.mosaic-backdrop
	display none
	position absolute
	top 2px
	left 2px
	height 128px
	width 128px
	background-color #444
	
.mosaic-overlay
	display none
	z-index 5
	position absolute
	width 100%
	height 100%
	background-color #555
	cursor pointer
	.details
		font-family Arial, Helvetica, sans-serif
		color #fff
		font-size 13px
		padding 4px
	.appname
		font-size 14px
		font-weight bold

.bar
	.mosaic-overlay
		bottom -132px
		height 164px

.mosaic-button
	position absolute
	bottom 6px
	left 20px

.button
.button:visited
	display block
	width 92px
	height 24px
	background-color #0079FF
	border-radius 2px
	text-align center
	line-height 24px
	text-decoration none
	color #fff
	cursor pointer
	&:hover
		box-shadow inset 0 0 4px #003979
		color #fff

body
	font-family CabinRegular
	font-size 16px
	background-color #333
	color #eee

#top
	width 670px
	margin-top 10px
	margin-bottom 10px
	margin-left auto
	margin-right auto
	position relative
	nav
		background-color #272727
		height 25px
		line-height 25px
		border-radius 4px
		.navlist
			li
				display inline
				list-style-type none
				padding-right 20px
				a, a:visited
					text-decoration none
					color #eee
					&:hover
						color #FFDA00
					
	
#header_logo
#header_logo:visited
	font-family FFFTusjBold
	font-size 72px
	display inline
	margin-left 4px
	margin-right 4px
	text-decoration none
	color #eee
	&:active
		color #FFDA00
	

#content
	width 670px
	margin-left auto
	margin-right auto

#bottom
	margin-top 1340px
